@Library('jenkins_shared_library') _ 
def COLOR_MAP = [
    'FAILURE' : 'Danger',
    'SUCCESS' : 'GOOD'

]

pipeline{
    agent any
    parameters{
        choice(name: 'action', choices: 'create\ndelete', description: 'Select Create or destroy')
    }
    stages{
        stage('clean workspace'){
            steps{
                cleanWorkspace()
            }
        }
        stage(checkout git){
            steps{
                checkoutGit('https://github.com/Aj7Ay/Youtube-clone-app.git', 'main')
            }
        }
    }
}